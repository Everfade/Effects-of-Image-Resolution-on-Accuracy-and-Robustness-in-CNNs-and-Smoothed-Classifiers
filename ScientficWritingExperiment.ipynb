    "    image_size=(x,y),\n",
    "    labels=\"inferred\",\n",
    "    shuffle=True,\n",
    "    validation_split=0,\n",
    "    color_mode=\"grayscale\",\n",
    "    label_mode=\"int\",\n",
    "    seed=seed)\n",
    "    return (concatenated_train_dataset, validation_ds,test_ds)\n",
    "    \n",
    "\n",
    "\n",
    "\n"
